		link(rel='stylesheet', href='css/demos/demo-law-firm.css')
		// Theme Custom CSS
		link(rel='stylesheet', href='css/custom.css')
		// Head Libs
		script(src='vendor/modernizr/modernizr.min.js')
	body
		.body
			header#header.header-no-border-bottom(data-plugin-options='{"stickyEnabled": true, "stickyEnableOnBoxed": true, "stickyEnableOnMobile": true, "stickyStartAt": 115, "stickySetTop": "-115px", "stickyChangeLogo": false}')
				.header-body
					.header-container.container
						.header-row
							.header-column
								.header-logo
									a(href='demo-law-firm.html')
										img(alt='Porto', width='164', height='54', data-sticky-width='82', data-sticky-height='40', data-sticky-top='33', src='img/demos/law-firm/logo-law-firm.png')
							.header-column
								ul.header-extra-info
									li
										.feature-box.feature-box-call.feature-box-style-2
											.feature-box-icon
												i.icon-call-end.icons
											.feature-box-info
												h4.mb-none (800) 123-4567
									li.hidden-xs
										.feature-box.feature-box-mail.feature-box-style-2
											.feature-box-icon
												i.icon-envelope.icons
											.feature-box-info
												h4.mb-none
													a(href='mailto:mail@example.com') mail@example.com
					.header-container.header-nav.header-nav-bar.header-nav-bar-primary
						.container
							button.btn.header-btn-collapse-nav(data-toggle='collapse', data-target='.header-nav-main')
								| Menu
								i.fa.fa-bars
							.header-search.visible-lg
								form#searchForm(action='page-search-results.html', method='get')
									.input-group
										input#q.form-control(type='text', name='q', placeholder='Search...', required='')
										span.input-group-btn
											button.btn.btn-default(type='submit')
												i.icon-magnifier.icons
							.header-nav-main.header-nav-main-light.header-nav-main-effect-1.header-nav-main-sub-effect-1.collapse
								nav
									ul#mainNav.nav.nav-pills
										li.active
											a(href='demo-law-firm.html') Home
										li
											a(href='demo-law-firm-about-us.html') About Us
										li
											a(href='demo-law-firm-attorneys.html') Attorneys
										li
											a(href='demo-law-firm-practice-areas.html') Practice Areas
										li
											a(href='demo-law-firm-news.html') News
										li
											a(href='demo-law-firm-contact-us.html') Contact Us
block content
block footer
			footer#footer.short
				.container
					.row
						.col-md-5
							a.logo.mb-md(href='demo-law-firm.html')
								img.img-responsive(alt='Porto Website Template', width='97', height='32', src='img/demos/law-firm/logo-law-firm-footer.png')
							p
								| Lorem ipsum dolor sit amet, consectetur adipiscing elit. Donec eu pulvinar magna. Phasellus semper scelerisque purus, et semper nisl lacinia sit amet. Praesent venenatis turpis vitae purus semper, eget sagittis velit venenatis.
						.col-md-3.col-md-offset-1
							h5.mb-sm Porto Law Firm
							ul.list.list-icons.mt-xl
								li
									i.fa.fa-map-marker
									strong Address:
									|  1234 Street Name, City Name, United States
								li
									i.fa.fa-envelope
									strong Email:
									a(href='mailto:mail@example.com') mail@example.com
						.col-md-3
							h5.mb-sm Toll Free
							span.phone (800) 123-4567
							ul.social-icons.mt-lg
								li.social-icons-facebook
									a(href='http://www.facebook.com/', target='_blank', title='Facebook')
										i.fa.fa-facebook
								li.social-icons-twitter
									a(href='http://www.twitter.com/', target='_blank', title='Twitter')
										i.fa.fa-twitter
								li.social-icons-linkedin
									a(href='http://www.linkedin.com/', target='_blank', title='Linkedin')
										i.fa.fa-linkedin
				.footer-copyright
					.container
						.row
							.col-md-12
								p
									| Â© Copyright 2016. All Rights Reserved.
									a(href='demo-law-firm-contact-us.html') Contact

